{
  "tailwindAttributes": ["variantConfig"],
  "tailwindFunctions": ["cn", "cva", "clsx", "twMerge", "tw"],
  "importOrder": [
    "^@core/(.*)$",
    "^@server/(.*)$",
    "^@ui/(.*)$",
    "^@/(.*)$",
    "^[./]"
  ],
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports"
  ]
}
